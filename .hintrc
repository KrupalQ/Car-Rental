{
  "extends": [
    "development"
  ],
  "hints": {
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/language": [
      "default",
      {
        "html-has-lang": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "frame-title": "off",
        "image-alt": "off"
      }
    ],
    "no-inline-styles": "off",
    "disown-opener": "off",
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "iframe[loading]"
        ]
      }
    ],
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "meta-viewport": "off",
    "compat-api/css": "off"
  }
}